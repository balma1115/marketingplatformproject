{
  "name": "marketingplat-lambda-functions",
  "version": "1.0.0",
  "description": "Lambda functions for MarketingPlat ranking tracking",
  "scripts": {
    "build": "tsc",
    "deploy": "serverless deploy --stage production",
    "deploy:dev": "serverless deploy --stage development",
    "package": "serverless package",
    "test": "jest",
    "build:layers": "npm run build:chromium-layer && npm run build:prisma-layer",
    "build:chromium-layer": "cd layers/chromium && npm install && cd ../..",
    "build:prisma-layer": "cd layers/prisma && npm install && npx prisma generate && cd ../.."
  },
  "dependencies": {
    "@prisma/client": "^6.16.2",
    "playwright-aws-lambda": "^0.10.0",
    "playwright-core": "^1.45.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.152",
    "@types/node": "^20.19.17",
    "copy-webpack-plugin": "^13.0.1",
    "prisma": "^6.16.2",
    "serverless": "^3.38.0",
    "serverless-dotenv-plugin": "^6.0.0",
    "serverless-offline": "^13.9.0",
    "serverless-plugin-include-dependencies": "^6.1.1",
    "serverless-plugin-typescript": "^2.1.5",
    "serverless-webpack": "^5.15.3",
    "ts-loader": "^9.5.4",
    "typescript": "^5.3.3",
    "webpack": "^5.101.3",
    "webpack-cli": "^6.0.1",
    "webpack-node-externals": "^3.0.0"
  }
}
