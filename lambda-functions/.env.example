# AWS Lambda Environment Variables

# Database Connection (AWS RDS)
DATABASE_URL="postgresql://username:password@your-rds-endpoint.amazonaws.com:5432/marketingplat"

# AWS Configuration
AWS_REGION="ap-northeast-2"

# Lambda VPC Configuration (필수)
LAMBDA_SECURITY_GROUP_ID="sg-xxxxxxxxxxxxxx"
LAMBDA_SUBNET_ID_1="subnet-xxxxxxxxxx"
LAMBDA_SUBNET_ID_2="subnet-yyyyyyyyyy"

# SQS Queue URLs (Serverless 배포 후 자동 생성)
SMARTPLACE_QUEUE_URL="https://sqs.ap-northeast-2.amazonaws.com/YOUR_ACCOUNT_ID/marketingplat-smartplace-queue-production"
BLOG_QUEUE_URL="https://sqs.ap-northeast-2.amazonaws.com/YOUR_ACCOUNT_ID/marketingplat-blog-queue-production"

# Optional: CloudWatch Settings
CLOUDWATCH_NAMESPACE="MarketingPlat/Tracking"